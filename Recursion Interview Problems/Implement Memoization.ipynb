   "source": [
    "### Memoization"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [],
   "source": [
    "memo = {}\n",
    "\n",
    "def fact(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    if n not in memo:\n",
    "        memo[n] = n*fact(n-1)\n",
    "    return memo[n] "
   ]
  },
